<% if post.liked_users.any? %>
  <%= post.liked_users.map{ |u| u.display_name}.join(",") %> Liked
<% end %>

<% if current_user %>
  <% if post.find_like(current_user) %>
    <%= link_to "Unlike", unlike_post_path(post), method: :post, remote: true, class: "btn btn-primary"%>
    <% else %>
    <%= link_to "Like", like_post_path(post), method: :post, remote: true, class:"btn btn-primary" %>
  <% end %>
<% end %>
